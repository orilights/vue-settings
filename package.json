{
  "name": "@orilight/vue-settings",
  "version": "1.2.0",
  "description": "LocalStorage wrapper for storing vue ref objects.",
  "keywords": [
    "vue",
    "settings"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/orilights/vue-settings.git"
  },
  "license": "MIT",
  "author": "OriLight",
  "type": "module",
  "exports": {
    ".": {
      "types": "./index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    }
  },
  "main": "dist/index.cjs.js",
  "module": "dist/index.esm.js",
  "types": "index.d.ts",
  "files": [
    "dist",
    "index.d.ts"
  ],
  "scripts": {
    "build": "rollup -c",
    "test": "vitest",
    "release": "npm run build && npm publish --registry=https://registry.npmjs.org/ --access public"
  },
  "dependencies": {
    "vue": "^3.3.4"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^25.0.4",
    "@rollup/plugin-json": "^6.0.0",
    "@rollup/plugin-node-resolve": "^15.2.1",
    "@types/node": "^20.5.9",
    "rollup": "^3.29.0",
    "rollup-plugin-dts": "^6.0.1",
    "rollup-plugin-esbuild": "^5.0.0",
    "typescript": "^5.1.6",
    "vitest": "^0.34.6"
  }
}
